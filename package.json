{
  "name": "wpglobus",
  "version": "2.1.2",
  "title": "WPGlobus",
  "description": "WPGlobus is a plugin for making bilingual / multilingual WordPress blogs and sites.",
  "private": true,
  "homepage": "https://wpglobus.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/WPGlobus/wpglobus.git"
  },
  "bugs": {
    "url": "https://github.com/WPGlobus/wpglobus/issues"
  },
  "author": "The WPGlobus Team <support@wpglobus.com>",
  "license": "GPL-3.0",
  "jshintConfig": {
    "node": true
  },
  "main": "Gulpfile.js",
  "devDependencies": {
    "fancy-log": "^1.3.3",
    "gulp": "^3.9.1",
    "gulp-append-prepend": "^1.0.5",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-downloader": "^1.0.4",
    "gulp-print": "^5.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.1",
    "gulp-vinyl-zip": "^2.1.2",
    "gulp-wp-pot": "^2.3.3",
    "plugin-error": "^1.0.1",
    "through2": "^3.0.0"
  },
  "dependencies": {},
  "scripts": {
    "list": "npm list --dept=0 && npm outdated",
    "audit": "npm audit",
    "upup": "del package-lock.json && del /s /q node_modules && npm install && npm run list",
    "changes": "git log --oneline develop --not master",
    "dist": "php /tools/composer.phar update && gulp dist",
    "push_pot": "tx push -s",
    "pull_translations": "tx pull -a -f --mode=translator",
    "pomo": "gulp pomo"
  }
}
